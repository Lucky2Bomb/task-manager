{
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["module-resolver",
    {
      "root": ["./src"],
      "alias": {
        "@": "./src"
      }
    }]
  ]
}
